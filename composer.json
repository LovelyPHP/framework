{
    "name": "lovelyphp/framework",
    "description": "the LovelyPHP Framework",
    "type": "framework",
    "keywords": [
        "framework",
        "lovelyphp",
        "ORM"
    ],
    "homepage": "https://lovelyphp.mojy.xyz",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "MoeCinnamo",
            "email": "abcd2890000456@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.28"
    },
    "require-dev": {
        "phpunit/phpunit": ">= 4.0 <9.5"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "lovely\\": "src/lovely/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "lovely\\tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}