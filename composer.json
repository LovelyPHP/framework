{
    "name": "lovelyphp/framework",
    "description": "the LovelyPHP Framework",
    "type": "framework",
    "keywords": [
        "framework",
        "lovelyphp",
        "ORM"
    ],
    "homepage": "https://lovelyphp.mojy.xyz",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "MoeCinnamo",
            "email": "abcd2890000456@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.40",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/log": "^1.0|^2.0|^3.0",
        "psr/container": "^1.0|^2.0",
        "psr/simple-cache": "^1.0|^2.0|^3.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": ">= 0.9.11 < 1.3.0",
        "phpunit/phpunit": ">= 4.0 < 9.6",
        "guzzlehttp/psr7": ">= 1.9 < 2.1.2"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "lovely\\": "src/lovely/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "lovely\\tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}